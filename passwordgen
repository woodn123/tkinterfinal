from tkinter import *

master = Tk()
v = StringVar()
master.wm_title("Password Generator Final Project")

b = Label(master, text='Password Generator', font=("Comic Sans MS",24))
b.pack()

z = Label(master, text="Length",font=("comic sans ms", 18))
z.pack()

a = Scale(master, from_=0, to=8, orient=HORIZONTAL, length=32*10)
length = IntVar()
length.set(8)
print(a.get())
a.pack()

mainloop()
